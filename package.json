{
  "name": "walk-apple-news-format",
  "version": "1.0.0",
  "description": "Walk the components in an apple news format JSON structure.",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava && semistandard | snazzy",
    "posttest": "readme package.json --travis > readme.md",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "watch": "rm -rf dist && mkdir -p dist && babel -w lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micnews/walk-apple-news-format.git"
  },
  "author": "mic.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micnews/walk-apple-news-format/issues"
  },
  "homepage": "https://github.com/micnews/walk-apple-news-format#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "package-json-to-readme": "^1.5.1",
    "semistandard": "^11.0.0",
    "snazzy": "^3.0.0"
  }
}
